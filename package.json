{
  "name": "natours",
  "version": "1.0.0",
  "description": "learning node / express / mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
